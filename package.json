  "license": "MIT",
  "repository": "slorber/awesome-debounce-promise",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "clean:all": "yarn clean && rm -rf yarn.lock node_modules",
